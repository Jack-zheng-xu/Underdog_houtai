<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.atguigu.eduservice.mapper.BlogEnjoyMapper">
    <select id="isEnjoy" resultType="java.lang.Integer">
        select count(*) from blog_enjoy where user_id=#{userId} and blog_id=#{blogId}
    </select>
</mapper>
